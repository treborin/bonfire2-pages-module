{
    "name": "dgvirtual/bonfire2-pages-module",
    "description": "A Pages module for Bonfire2",
    "type": "library",
    "require-dev": {
        "codeigniter4/devkit": "^1.3",
        "codeigniter4/framework": "^4.3",
        "lonnieezell/bonfire": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\Pages\\": "Pages/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
